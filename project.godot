; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Villainess x Reborn"
run/main_scene="res://scenes/menus/title.tscn"
config/features=PackedStringArray("4.4", "Forward Plus")
run/max_fps=144
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://assets/misc/IMG_4946.jpeg"

[autoload]

EffectAnim="*res://tools/effect/effect_anim.tscn"
Global="*res://global/global.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
Util="*res://global/utilities.gd"
WindowDimensionHelper="*res://global/window_dimension_helper.gd"
EffectReg="*res://global/EffectReg.gd"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/shadowed_variable=0
gdscript/warnings/shadowed_variable_base_class=0
gdscript/warnings/incompatible_ternary=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=3

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/godotsize/plugin.cfg", "res://addons/wigglebone/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://tools/minigames/": "yellow",
"res://tools/minigames/fishing/": "pink",
"res://tools/minigames/library/": "orange",
"res://tools/minigames/maze/": "teal",
"res://tools/minigames/shark_tank/": "green",
"res://tools/minigames/social_warfare/": "blue",
"res://tools/minigames/template/": "gray"
}

[global_group]

Player="Group of Nodes that are Characters controlled by the Player"
Ground=""
FallingItems=""

[gui]

theme/custom_font="res://assets/ui/ttf/Volkhov-Regular.ttf"
theme/default_font_hinting=2
theme/default_font_multichannel_signed_distance_field=true
theme/default_font_generate_mipmaps=true

